{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "no-console": false,
        "interface-name": false,
        "semicolon": false,
        "jsx-no-multiline-js": false,
        "object-literal-sort-keys": false,
        "no-empty": false,
        "max-line-length":  [true, 150],
        "jsx-boolean-value": false,
        "no-duplicate-imports": false,
        "no-submodule-imports": false
    }
}